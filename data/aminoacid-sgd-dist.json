{
    "solver-method": "gcp-sgd-dist",
    "tensor":
    {
        "input-file": "data/aminoacid_data.txt",
        "index-base": 0
    },
    "k-tensor":
    {
        "rank": 4,
        "output": "aminoacid.ktn",
        "initial-guess": "rand",
        "distributed-guess": "parallel-drew",
        "seed": 12345,
        "prng": false
    },
    "gcp-sgd-dist":
    {
        "method": "adam",
        "loss": "gaussian",
        "batch-size-nz": 1000,
        "batch-size-zero": 0,
        "max-epochs": 100,
        "learning-rate":
        {
            "step": 1e-3
        }
    }
}
